{
    "name": "aoeng/laravel-workerman",
    "description": "laravel workerman websocket",
    "keywords": [
        "laravel",
        "workerman",
        "websocket"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dang",
            "email": "yofengge@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Aoeng\\Laravel\\Workerman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aoeng\\Laravel\\Workerman\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aoeng\\Laravel\\Workerman\\WorkermanServiceProvider"
            ],
            "aliases": {
                "Gateway": "Aoeng\\Laravel\\Workerman\\Facades\\Gateway"
            }
        }
    }
}
